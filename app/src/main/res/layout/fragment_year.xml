<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/gradient1"
    tools:context=".Fragment.YearFragment">

    <com.example.todoido.AnimeView.FlowerView
        android:id="@+id/flowerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <com.example.todoido.AnimeView.RainView
        android:id="@+id/rainView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <com.example.todoido.AnimeView.LeaveView
        android:id="@+id/leaveView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <com.example.todoido.AnimeView.SnowView
        android:id="@+id/snowView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/year_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이번년도 버킷리스트"
        android:gravity="center"
        android:textSize="19dp"
        android:padding="5dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/selected_tab_text_color"/>


    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:id="@+id/yearlayout"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/year_txt"
        android:orientation="vertical"
        android:background="?attr/year_bg">

        <include layout="@layout/year_item" android:id="@+id/item1" />
        <include layout="@layout/year_item" android:id="@+id/item2" />
        <include layout="@layout/year_item" android:id="@+id/item3" />
        <include layout="@layout/year_item" android:id="@+id/item4" />
        <include layout="@layout/year_item" android:id="@+id/item5" />
        <include layout="@layout/year_item" android:id="@+id/item6" />
        <include layout="@layout/year_item" android:id="@+id/item7" />
        <include layout="@layout/year_item" android:id="@+id/item8" />
        <include layout="@layout/year_item" android:id="@+id/item9" />
        <include layout="@layout/year_item" android:id="@+id/item10" />

    </LinearLayout>

</RelativeLayout>
